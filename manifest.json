{
    "name": "Marketplace Search",
    "description": "Quick marketplace search in one tab",
    "version": "1.4",
    "manifest_version": 3,
    "icons": {
        "16":"icons/icon-16x16.png",
        "32":"icons/icon-32x32.png",
        "48":"icons/icon-48x48.png",
        "128":"icons/icon-128x128.png"
    },
    "permissions": [
        "storage",
        "scripting", 
        "activeTab"
    ],
    "action": {
        "default_popup":"popup.html"
    }
}